Proceso Operaciones_punto4
	Definir n,cuadrado,cubo,doble,triple Como Real;
	Escribir 'Ingrese un numero';
	Leer n;
	cuadrado <- n*n;
	cubo <- n*n*n;
	doble <- n*2;
	triple <- n*3;
	Escribir 'El cuadrado del numero es ',cuadrado;
	Escribir 'El cubo del numero es ',cubo;
	Escribir 'El doble del numero es ',doble;
	Escribir 'El triple del numero es ',triple;
FinProceso

